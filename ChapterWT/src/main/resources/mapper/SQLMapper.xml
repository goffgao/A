<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 命名空间mapper,如果有多个mapper文件，这个必须唯一 -->
<mapper namespace="cn.wt.model">

    <!--获取登陆接口case-->
    <select id="DevelopHallNameCase" parameterType="Integer"  resultType="com.tester.model.DevelopHallNameCase">

        select * from DevelopHallNameCase
        where id = #{id};
    </select>


</mapper>